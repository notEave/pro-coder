{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "./js",
        "newLine": "lf",
        "charset": "utf8",
        "removeComments": true,

        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": false,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "allowJs": false,
        "sourceMap": false
    }
}
